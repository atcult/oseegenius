		#if ($resource.scopeNote)<a class="bottom-toolbar-link" href="javascript:goTo('scopeNote')">$text.scope_note</a>#end
		#if ($resource.editorialNote)<a class="bottom-toolbar-link" href="javascript:goTo('editorialNote')">$text.editorial_note</a>#end
		#if ($resource.historyNote)<a class="bottom-toolbar-link" href="javascript:goTo('historyNote')">$text.history_notes</a>#end
		#if ($resource.note)<a class="bottom-toolbar-link" href="javascript:goTo('note')">$text.note</a>#end
		#if ($resource.broader || $resource.narrower || $resource.related)<a class="bottom-toolbar-link" href="javascript:goTo('relations')">$text.relations</a>#end
