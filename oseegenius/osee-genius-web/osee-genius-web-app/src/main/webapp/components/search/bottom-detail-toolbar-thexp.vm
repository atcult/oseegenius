<div id="toolbar" class="box">
	<span style="float:left;">#spacer(10)</span>
	<span style="float:left;margin-bottom:5px;">#spacer(10)</span>
	<div id="navigator" style="padding-top:4px;">
		<a class="bottom-toolbar-link" href="javascript:goTo('header')">$text.general</a>
		#if ($resource.in_scheme)<a class="bottom-toolbar-link" href="javascript:goTo('schemes')">$text.scheme_name</a>#end
